{
  "manifest_version": 3,
  "name": "text2latex",
  "version": "1.0.0",
  "author": "Aarushi Sachdeva",
  "description": "turn natural lanauge into latex code with no hassle",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": [
    {
      "default_popup": "index.html",
      "default_title": "text2latex"
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://overleaf.com/project/*"],
      "js": ["contentScript.js"]
    }
  ]
}
